h3,
h4,
h5,
h6 {
  clear: both;
  margin: 0 0 0.6em 0; }

dfn, cite, em, i {
  font-style: italic; }

pre {
  background: #e9ecef;
  font-family: "Courier 10 Pitch", Courier, monospace;
  line-height: 1.2;
  max-width: 100%;
  padding: 1.2em; }

code,
kbd,
tt,
var {
  font-family: Monaco, Consolas, "Andale Mono", "DejaVu Sans Mono", monospace; }

